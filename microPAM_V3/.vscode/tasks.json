{ 
  "command": "${workspaceFolder}/../make.exe",
  "config": "${command:cpptools.activeConfigName}",
    "tasks": [
    {
      "label": "Build",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "args": [
        "all",
        "-j",
        "-Otarget",
        "CONFIG_NAME=${command:cpptools.activeConfigName}"
      ]
    },
    {
      "label": "Rebuild User Code",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "args": [
        "rebuild",
        "-j",
        "-Otarget",
        "CONFIG_NAME=${command:cpptools.activeConfigName}"
      ]
    },
    {
      "label": "Clean",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "args": [
        "clean",
        "CONFIG_NAME=${command:cpptools.activeConfigName}"
      ]
    },
    {
      "label": "Upload (Teensy Uploader)",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "args": [
        "upload",
        "-j",
        "-Otarget",
        "CONFIG_NAME=${command:cpptools.activeConfigName}"
      ]
    }
    ,
    {
      "label": "Terminal",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "args": [
        "terminal",
        "-j",
        "-Otarget"
      ]
    }
    ,
    {
      "label": "Document",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "args": [
        "document",
        "-j",
        "-Otarget",
        "CONFIG_NAME=${command:cpptools.activeConfigName}"
      ]
    }],
  "version": "2.0.0",
  "type": "shell",
  "presentation": {
    "echo": true,
    "reveal": "always",
    "focus": false,
    "panel": "shared",
    "showReuseMessage": false
  },
  "problemMatcher": "$gcc"
}